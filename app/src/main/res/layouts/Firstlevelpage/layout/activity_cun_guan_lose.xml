<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yixingjjinrong.yixinjinrongapp.wode.yinghancunguan_jieguo.CunGuan_lose">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="138px"
        android:background="@drawable/mybackgrandcolor"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/kt_lose_fh"
            android:layout_width="40px"
            android:layout_height="match_parent"
            android:layout_marginLeft="20px"
            android:paddingRight="20px"
            android:paddingTop="20px"
            android:src="@drawable/fanhui" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:paddingTop="60px"
            android:text="开通失败"
            android:textColor="#ffffff"
            android:textSize="38px" />
    </RelativeLayout>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/shibai"
        android:layout_gravity="center"
        android:layout_marginTop="189px"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#333"
        android:textSize="44px"
        android:text="开通失败"
        android:layout_gravity="center"
        android:layout_marginTop="27px"
        />
    <Button
        android:id="@+id/kt_lose_fanhui"
        android:layout_width="490px"
        android:layout_height="90px"
        android:background="@drawable/bt_biankuang"
        android:text="返回"
        android:layout_marginTop="90px"
        android:layout_gravity="center"
        android:textColor="#f46d14" />
    <Button
        android:id="@+id/kt_lose_mycontent"
        android:layout_width="490px"
        android:layout_height="90px"
        android:background="@drawable/bt_shape"
        android:text="账户中心"
        android:layout_marginTop="36px"
        android:layout_gravity="center"
        android:textColor="#fdfdfe" />
</com.zhy.autolayout.AutoLinearLayout>
