<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yixingjjinrong.yixinjinrongapp.wode.fore_inot.wodechujie_one.MyChuJie_one">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="138px"
        android:background="@drawable/mybackgrandcolor"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/chujie_one_fh"
            android:layout_width="40px"
            android:layout_height="match_parent"
            android:layout_marginLeft="20px"
            android:paddingRight="20px"
            android:paddingTop="20px"
            android:src="@drawable/fanhui" />

        <TextView
            android:layout_width="260px"
            android:layout_height="match_parent"
            android:layout_marginLeft="251px"
            android:gravity="center_horizontal"
            android:paddingTop="60px"
            android:text="我的出借"
            android:textColor="#ffffff"
            android:textSize="38px" />
    </RelativeLayout>

    <com.jcodecraeer.xrecyclerview.XRecyclerView
        android:id="@+id/chujie_one_xview"
        android:layout_width="match_parent"
        android:layout_height="1100px"></com.jcodecraeer.xrecyclerview.XRecyclerView>

    <TextView
        android:id="@+id/yidaoqixiangmu"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="已到期项目  >>"
        android:gravity="center"
        android:textSize="24px"
        android:textColor="#999999"
        />
</com.zhy.autolayout.AutoLinearLayout>
