<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yixingjjinrong.yixinjinrongapp.wode.fore_inot.juan_fragment.juan_daoqi.JiaxiJuan_DaoQi">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="118px"
        android:layout_marginTop="20px"
        android:background="#fdfdfe"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/ys_xjfh"
            android:layout_width="40px"
            android:layout_height="match_parent"
            android:layout_marginLeft="20px"
            android:paddingRight="20px"
            android:paddingTop="20px"
            android:src="@drawable/jiantou" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:gravity="center_horizontal"
            android:paddingTop="60px"
            android:text="已使用加息券"
            android:textColor="#363636"
            android:textSize="38px" />
    </RelativeLayout>
    <include
        android:id="@+id/jx_wsys"
        layout="@layout/wushiyong_jxj"
        android:visibility="gone"
        />
    <com.jcodecraeer.xrecyclerview.XRecyclerView
        android:id="@+id/ys_xj_xview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.jcodecraeer.xrecyclerview.XRecyclerView>
</com.zhy.autolayout.AutoLinearLayout>
