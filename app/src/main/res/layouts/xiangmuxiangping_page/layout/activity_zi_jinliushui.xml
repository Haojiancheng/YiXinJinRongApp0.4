<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".wode.fore_inot.zijinliushui.ZiJinliushui">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="118px"
        android:background="@drawable/mybackgrandcolor"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/wo_zjls_fh"
            android:layout_width="40px"
            android:layout_height="match_parent"
            android:src="@drawable/fanhui"
            android:paddingTop="20px"
            android:paddingRight="20px"
            android:layout_marginLeft="20px"
            />

        <TextView
            android:layout_width="260px"
            android:layout_height="match_parent"
            android:layout_marginLeft="251px"
            android:gravity="center_horizontal"
            android:paddingTop="60px"
            android:text="资金流水"
            android:textColor="#ffffff"
            android:textSize="38px" />

    </RelativeLayout>
    <include
        android:id="@+id/zijinliushui_wushuju"
        layout="@layout/wujilu_zijinliushui"
        android:visibility="gone"
        />
    <com.jcodecraeer.xrecyclerview.XRecyclerView
        android:id="@+id/liushuirview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.jcodecraeer.xrecyclerview.XRecyclerView>
</com.zhy.autolayout.AutoLinearLayout>