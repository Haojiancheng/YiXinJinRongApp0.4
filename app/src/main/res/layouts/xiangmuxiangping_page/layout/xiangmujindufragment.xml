<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0f0f0"
    android:orientation="vertical">
    <include
        android:id="@+id/jd_dengruchakan"
        layout="@layout/dengru_chakan"
        android:visibility="gone"
        />

    <com.yixingjjinrong.yixinjinrongapp.xiangmuyemian.Xiangmuxiangqing.xiangqing.myview.MyScrollView
        android:id="@+id/xiangmujingduScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20px">


        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="54px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/fabuxiangmu"
                    android:layout_width="0px"
                    android:layout_height="70px"
                    android:layout_weight="1"
                    android:src="@drawable/fabuxiangmu" />

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="发布项目"
                        android:textColor="#333333"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/fabuxiangmu_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20px"
                        android:text="开始时间  2018-01-15 17:30:00"
                        android:textColor="#999999"
                        android:textSize="24px" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/fb_xyb"
                    android:layout_width="0px"
                    android:layout_height="50px"
                    android:layout_weight="1"
                    android:src="@drawable/jinxingwancheng" />

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/muji"
                    android:layout_width="0px"
                    android:layout_height="70px"
                    android:layout_weight="1"
                    android:src="@drawable/mujizhong" />

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="募集完成"
                        android:textColor="#333333"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/mujitime"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20px"
                        android:text="募集完成  正在募集中..."
                        android:textColor="#999999"
                        android:textSize="24px" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/muji_xyb"
                    android:layout_width="0px"
                    android:layout_height="50px"
                    android:layout_weight="1"
                    android:src="@drawable/jinxingngzhong" />

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/fangkuan"
                    android:layout_width="0px"
                    android:layout_height="70px"
                    android:layout_weight="1"
                    android:src="@drawable/fangkuanzhong" />

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="项目放款"
                        android:textColor="#333333"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/fangkuan_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20px"
                        android:text="开始时间  募集完成放款后起息"
                        android:textColor="#999999"
                        android:textSize="24px" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/fk_xyb"
                    android:layout_width="0px"
                    android:layout_height="50px"
                    android:layout_weight="1"
                    android:src="@drawable/jinxingngzhong" />

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"></LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30px"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/huankuan"
                    android:layout_width="0px"
                    android:layout_height="70px"
                    android:layout_weight="1"
                    android:src="@drawable/huankuanzhomg" />

                <LinearLayout
                    android:layout_width="0px"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="项目还款"
                        android:textColor="#333333"
                        android:textSize="28px" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/yihuan_jine"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20px"
                            android:text="已还金额 0.00"
                            android:textColor="#999999"
                            android:textSize="24px" />

                        <TextView
                            android:id="@+id/yehuan_qi"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="8px"
                            android:layout_marginTop="20px"
                            android:text="(0期)"
                            android:textColor="#999999"
                            android:textSize="24px" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/daihuan_jine"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20px"
                            android:text="待还金额 0.00"
                            android:textColor="#999999"
                            android:textSize="24px" />

                        <TextView
                            android:id="@+id/daihua_qi"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="8px"
                            android:layout_marginTop="20px"
                            android:text="(12期)"
                            android:textColor="#999999"
                            android:textSize="24px" />
                    </LinearLayout>


                </LinearLayout>


            </LinearLayout>
        </LinearLayout>

    </com.yixingjjinrong.yixinjinrongapp.xiangmuyemian.Xiangmuxiangqing.xiangqing.myview.MyScrollView>


</com.zhy.autolayout.AutoLinearLayout>